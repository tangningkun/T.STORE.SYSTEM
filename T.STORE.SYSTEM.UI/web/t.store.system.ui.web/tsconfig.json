{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "esnext",
    "typeRoots": [
      "node_modules/@types"
    ],
    // "target": "es2015",
    // "module": "es2015",
    // "moduleResolution": "node",
    // "experimentalDecorators": true,
    // "emitDecoratorMetadata": true,
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "esModuleInterop": true,
    // "allowSyntheticDefaultImports": true,
    // "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    // "lib": [
    //   "es2015",
    //   "dom",
    //   "dom.iterable",
    //   "scripthost"
    // ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}